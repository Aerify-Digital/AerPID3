{
  "name": "aertiny",
  "version": "3.3.0",
  "description": "ESP32 PID Controller for Heating Element",
  "scripts": {
    "setup-electron": "npm install --prefix ./app/electron",
    "start-electron": "npm run start --prefix ./app/electron",
    "package-electron": "npm run package --prefix ./app/electron",
    "make-electron": "npm run make --prefix ./app/electron",
    "publish-electron": "npm run publish --prefix ./app/electron",
    "lint-electron": "npm run lint --prefix ./app/electron",
    "minify": "node ./scripts/minify.mjs ./",
    "version": "bash ./scripts/version.sh ./",
    "update": "bash ./scripts/aerpid.sh update ",
    "upload-1": "bash ./scripts/aerpid.sh upload-1",
    "upload-2": "bash ./scripts/aerpid.sh upload-2",
    "upgrade-1": "bash ./scripts/aerpid.sh upgrade 1",
    "upgrade-2": "bash ./scripts/aerpid.sh upgrade 2",
    "devices":  "bash ./scripts/aerpid.sh devices",
    "deploy": "bash ./scripts/deploy.sh all"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Aerify-Digital/AerPID3.git"
  },
  "author": "AerifyDigital",
  "contributors": [
    {
      "name": "SeqSEE"
    },
    {
      "name": "squidicuz"
    },
    {
      "name": "Boehmerang"
    }
  ],
  "license": "GPL-3.0-only",
  "devDependencies": {
    "html-minifier-terser": "^7.2.0",
    "terser": "^5.37.0"
  }
}
